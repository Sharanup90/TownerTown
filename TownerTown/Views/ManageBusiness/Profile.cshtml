                EndTime: saturdayEndTime,
                Closed: saturdayClose
            },
            Sunday: {
                StartTime: sundayStartTime,
                EndTime: sundayEndTime,
                Closed: sundayClose
            }
        }

        if (validated == true) {
            $.ajax({
                url: "/ManageBusiness/UpdateTimings",
