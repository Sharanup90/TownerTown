@model TownerTown.Web.Models.RegisterViewModel

@{
    ViewData["Title"] = "ViewModel";
}
<div style="background-color:white">

    <div style="background-color:white">
        <div>
            @if (Model.Business.Images.Count != 0)
            {
                <img src="@Url.Content(Model.Business.Images[0].Path)" style="width:100%; height:250px" />
            }
            else
            {
                <img src="~/lib/images/ShopImages/mainImg.jpg" style="width:100%; height:250px" />
            }
        </div>
        <div class="fixed navbar navbar">
            <a class="active" href="#">Overview</a>
            <a href="#About">About</a>
            <a href="#Products">Products</a>
            <a href="#Reviews">Reviews</a>
            <a href="#Photos">Photos</a>
        </div>

        <div class="col-sm-12">

            <div style="padding-right:0px">
                <div class="container-fluid">
                    <form name="updateModelform">
                        <div class="row">

                            <div class="col-sm-12" style="margin:0px;padding:0px">
                                <h3 style="color:blue; font-size:24px">Overview</h3>
                            </div>
                            <div class="col-sm-10" style="margin:0px;padding:0px">
                                <h4 id="ModelName1" style="color:brown">@Model.Business.BusinessName </h4>
                                <input type="text" id="ModelName2" class="form-control" value="@Model.Business.BusinessName" style="width:700px; display:none" />
                            </div>

                            <div class="col-sm-2" style="margin:0px;padding:0px">
                                <input id="editModelButton" type="button" onclick="EditBussinessName()" class="btn btn-light" value="Edit" style="" />
                                <input id="updateModelButton" type="button" onclick="UpdateBussinessName()" class="btn btn-primary" value="Update" style="display:none" />
                            </div>

                            <div class="col-sm-12" style="margin:0px;padding:0px">
                                <hr />
                            </div>
                            <div class="col-sm-10" style="margin:0px;padding:0px">
                                <h5 id="TagLine1">@Model.Business.TagLine </h5>
                                <input type="text" id="TagLine2" class="form-control" value="@Model.Business.TagLine" style="width:700px; display:none" />
                            </div>
                            <div class="col-sm-2" style="margin:0px;padding:0px">
                                <input id="editTagLineButton" type="button" onclick="EditTagLine()" class="btn btn-light" value="Edit" style="" />
                                <input id="updateTagLineButton" type="button" onclick="UpdateTagLine()" class="btn btn-primary" value="Update" style="display:none" />
                            </div>
                            <div class="col-sm-12" style="margin:0px;padding:0px">
                                <hr />
                            </div>
                            <div class="col-sm-2" style="margin:0px;padding:0px">
                                Category:
                            </div>
                            <div class="col-sm-8" id="Category1" style="margin:0px;padding:0px">
                                <h6>@Model.Business.Category.CategoryName </h6>
                            </div>
                            <div class="col-sm-8" id="Category2" style="margin:0px;padding:0px; display:none">
                                @Html.ListBoxFor(Model => Model.SearchViewModel.selectedCategories, new SelectList(Model.SearchViewModel.Categories, "CategoryName", "CategoryName"), new { Id = "Categories", multiple = "multiple" })
                            </div>
                            <div class="col-sm-2" style="margin:0px;padding:0px">
                                <input id="editCategoryButton" type="button" onclick="EditCategory()" class="btn btn-light" value="Edit" style="" />
                                <input id="updateCategoryButton" type="button" onclick="UpdateCategory()" class="btn btn-primary" value="Update" style="display:none" />
                            </div>
                            <div class="col-sm-12" style="margin:0px;padding:0px">
                                <hr />
                            </div>
                            <div class="col-sm-2" style="margin:0px;padding:0px">
                                WebSite Link:
                            </div>
                            <div class="col-sm-8" style="margin:0px;padding:0px">
                                @if (Model.Business.WebsiteLink == null || Model.Business.WebsiteLink == "")
                                {
                                    <input type="text" id="AddWebsite" class="form-control" style="width:700px;" />
                                }
                                else
                                {
                                    <h6 id="WebsiteLink1">@Model.Business.WebsiteLink </h6>
                                }

                                <input type="text" id="WebsiteLink2" class="form-control" value="@Model.Business.WebsiteLink" style="width:700px; display:none" />

                            </div>
                            <div class="col-sm-2" style="margin:0px;padding:0px">
                                @if (Model.Business.WebsiteLink == null || Model.Business.WebsiteLink == "")
                                {
                                    <input id="AddWebsiteLinkButton" type="button" onclick="AddWebsiteLink()" class="btn btn-light" value="Edit" style="" />
                                }
                                else
                                {
                                    <input id="editWebsiteLinkButton" type="button" onclick="EditWebsiteLink()" class="btn btn-light" value="Edit" style="" />
                                    <input id="updateWebsiteLinkButton" type="button" onclick="UpdateWebsiteLink()" class="btn btn-primary" value="Update" style="display:none" />
                                }
                            </div>
                            <hr />

                            <div class="col-sm-12" style="margin:0px;padding:0px">
                                <hr />
                            </div>
                            <div class="col-sm-2" style="padding: 0px;">
                                Address:
                            </div>
                            <div id="Address1" class="col-sm-8" style="margin:0px;padding:0px">
                                <h6>@Model.Business.Address.AddressLine1</h6>
                                <h6>@Model.Business.Address.AddressLine2</h6>
                                <h6> @Model.Business.Address.Area, @Model.Business.Address.City, @Model.Business.Address.Country, @Model.Business.Address.PinCode</h6>
                                <div style="/* width:40px; */height:40px;">View On Map<img style="width:40px;height: 35px;margin-top: -7px;" src="/lib/images/Logos/location.jpg"></div>
                            </div>
                            <div id="Address2" class="col-sm-8" style="display: none">
                                <div class="form-group">
                                    <label>Address</label>
                                    <textarea required placeholder="Enter Address Here.." asp-for="@Model.Business.Address.AddressLine1" rows="3" class="form-control"></textarea>
                                </div>
                                <div class="row">
                                    <div class="col-sm-3 form-group">
                                        <label>Area</label>
                                        <input required type="text" asp-for="@Model.Business.Address.Area" placeholder="Enter Area Name Here.." class="form-control">
                                    </div>
                                    <div class="col-sm-3 form-group">
                                        <label>City</label>
                                        <input required type="text" asp-for="@Model.Business.Address.City" placeholder="Enter City Name Here.." class="form-control">
                                    </div>
                                    <div class="col-sm-3 form-group">
                                        <label>State</label>
                                        <input required asp-for="@Model.Business.Address.State" placeholder="Choose State" list="states" name="state" id="state" class="form-control">
                                        <datalist id="states">
                                            <option value="">------------Select State------------</option>
                                            <option value="Andaman and Nicobar Islands">Andaman and Nicobar Islands</option>
                                            <option value="Andhra Pradesh">Andhra Pradesh</option>
                                            <option value="Arunachal Pradesh">Arunachal Pradesh</option>
                                            <option value="Assam">Assam</option>
                                            <option value="Bihar">Bihar</option>
                                            <option value="Chandigarh">Chandigarh</option>
                                            <option value="Chhattisgarh">Chhattisgarh</option>
                                            <option value="Dadra and Nagar Haveli">Dadra and Nagar Haveli</option>
                                            <option value="Daman and Diu">Daman and Diu</option>
                                            <option value="Delhi">Delhi</option>
                                            <option value="Goa">Goa</option>
                                            <option value="Gujarat">Gujarat</option>
                                            <option value="Haryana">Haryana</option>
                                            <option value="Himachal Pradesh">Himachal Pradesh</option>
                                            <option value="Jammu and Kashmir">Jammu and Kashmir</option>
                                            <option value="Jharkhand">Jharkhand</option>
                                            <option value="Karnataka">Karnataka</option>
                                            <option value="Kerala">Kerala</option>
                                            <option value="Lakshadweep">Lakshadweep</option>
                                            <option value="Madhya Pradesh">Madhya Pradesh</option>
                                            <option value="Maharashtra">Maharashtra</option>
                                            <option value="Manipur">Manipur</option>
                                            <option value="Meghalaya">Meghalaya</option>
                                            <option value="Mizoram">Mizoram</option>
                                            <option value="Nagaland">Nagaland</option>
                                            <option value="Orissa">Orissa</option>
                                            <option value="Pondicherry">Pondicherry</option>
                                            <option value="Punjab">Punjab</option>
                                            <option value="Rajasthan">Rajasthan</option>
                                            <option value="Sikkim">Sikkim</option>
                                            <option value="Tamil Nadu">Tamil Nadu</option>
                                            <option value="Tripura">Tripura</option>
                                            <option value="Uttaranchal">Uttaranchal</option>
                                            <option value="Uttar Pradesh">Uttar Pradesh</option>
                                            <option value="West Bengal">West Bengal</option>
                                            <option value="Ladakh">Ladakh</option>
                                            <option value="Kashmir">Kashmir</option>
                                        </datalist>
                                        <input type="text" asp-for="@Model.Business.Address.State" hidden />
                                        <span asp-validation-for="@Model.Business.Address.State" class="text-danger"></span>
                                    </div>
                                    <input type="text" asp-for="@Model.Business.Address.Country" hidden />
                                    <div class="col-sm-3 form-group">
                                        <label>PinCode</label>
                                        <input required pattern="[0-9]{4}" maxlength="6" type="number" asp-for="@Model.Business.Address.PinCode" placeholder="Enter PinCode Here.." class="form-control">
                                        <span asp-validation-for="@Model.Business.Address.PinCode" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-2" style="margin:0px;padding:0px">
                                <input id="editAddressButton" type="button" onclick="EditAddress()" class="btn btn-light" value="Edit" style="" />
                                <input id="updateAddressButton" type="button" onclick="UpdateAddress()" class="btn btn-primary" value="Update" style="display:none" />
                            </div>
                            <div class="col-sm-12" style="margin:0px;padding:0px">
                                <hr />
                            </div>

                            <div class="col-sm-2" style="margin-top: 10px;padding: 0px;">
                                Mobile Number:
                            </div>
                            <div class="col-sm-2" style="margin-top: 10px;">
                                <h6 id="MobileNumber1">@Model.Business.BusinessOwner.MobileNumber </h6>
                                <input type="text" id="MobileNumber2" class="form-control" value=">@Model.Business.BusinessOwner.MobileNumber" style="display:none" />
                            </div>
                            <div class="col-sm-2" style="margin:0px;padding:0px">
                                <input id="editMobileNumberButton" type="button" onclick="EditMobileNumber()" class="btn btn-light" value="Edit" style="" />
                                <input id="updateMobileNumberButton" type="button" onclick="UpdateMobileNumber()" class="btn btn-primary" value="Update" style="display:none" />
                            </div>
                            <div class="col-sm-2" style="margin-top: 10px;padding: 0px;">
                                WhatsApp Number:
                            </div>
                            <div class="col-sm-2" style=" margin-top: 10px;">
                                <h6 id="WhatsAppNumber1">@Model.Business.BusinessOwner.WhatsAppNumber </h6>
                                <input type="text" id="WhatsAppNumber2" class="form-control" value=">@Model.Business.BusinessOwner.WhatsAppNumber" style="display:none" />
                            </div>

                            <div class="col-sm-2" style="margin:0px;padding:0px">
                                <input id="editWhatsAppNumberButton" type="button" onclick="EditWhatsAppNumber()" class="btn btn-light" value="Edit" style="" />
                                <input id="updateWhatsAppNumberButton" type="button" onclick="UpdateWhatsAppNumber()" class="btn btn-primary" value="Update" style="display:none" />
                            </div>
                            <div class="col-sm-12" style="margin:0px;padding:0px">
                                <hr />
                            </div>
                            <div>

                            </div>
                            <div class="col-sm-2" style=" margin-top: 10px;padding:0px">
                                <h6>Timings:</h6>
                            </div>
                            <div id="Timings1" class="row">
                                <div class="col-sm-2" style=" margin-top: 10px;">

                                </div>
                                <div class="col-sm-2" style=" margin-top: 10px;">
                                    <h6>Monday</h6>
                                </div>
                                @if (Model.Business.Timings.Monday.Closed != true)
                                {
                                    <div class="col-sm-8" style=" margin-top: 10px;">
                                        @Model.Business.Timings.Monday.StartTime To  @Model.Business.Timings.Monday.EndTime
                                    </div>
                                }
                                else
                                {
                                    <div class="col-sm-8">
                                        Closed
                                    </div>
                                }

                                <div class="col-sm-2" style=" margin-top: 10px;padding:0px">

                                </div>
                                <div class="col-sm-2" style=" margin-top: 10px;">
                                    <h6>Tuesday</h6>
                                </div>
                                @if (Model.Business.Timings.Tuesday.Closed != true)
                                {
                                    <div class="col-sm-8" style=" margin-top: 10px;">
                                        @Model.Business.Timings.Tuesday.StartTime To  @Model.Business.Timings.Tuesday.EndTime
                                    </div>
                                }
                                else
                                {
                                    <div class="col-sm-8">
                                        Closed
                                    </div>
                                }
                                <div class="col-sm-2" style=" margin-top: 10px;padding:0px">
                                </div>
                                <div class="col-sm-2" style=" margin-top: 10px;">
                                    <h6>Wednesday</h6>
                                </div>
                                @if (Model.Business.Timings.Wednesday.Closed != true)
                                {
                                    <div class="col-sm-8" style=" margin-top: 10px;">
                                        @Model.Business.Timings.Wednesday.StartTime To  @Model.Business.Timings.Wednesday.EndTime
                                    </div>
                                }
                                else
                                {
                                    <div class="col-sm-8">
                                        Closed
                                    </div>
                                }
                                <div class="col-sm-2" style=" margin-top: 10px;padding:0px">
                                </div>
                                <div class="col-sm-2" style=" margin-top: 10px;">
                                    <h6>Thursday</h6>
                                </div>
                                @if (Model.Business.Timings.Thursday.Closed != true)
                                {
                                    <div class="col-sm-8" style=" margin-top: 10px;">
                                        @Model.Business.Timings.Thursday.StartTime To  @Model.Business.Timings.Thursday.EndTime
                                    </div>
                                }
                                else
                                {
                                    <div class="col-sm-8">
                                        Closed
                                    </div>
                                }
                                <div class="col-sm-2" style=" margin-top: 10px;padding:0px">
                                </div>
                                <div class="col-sm-2" style=" margin-top: 10px;">
                                    <h6>Friday</h6>
                                </div>
                                @if (Model.Business.Timings.Friday.Closed != true)
                                {
                                    <div class="col-sm-8" style=" margin-top: 10px;">
                                        @Model.Business.Timings.Friday.StartTime To  @Model.Business.Timings.Friday.EndTime
                                    </div>
                                }
                                else
                                {
                                    <div class="col-sm-8">
                                        Closed
                                    </div>
                                }
                                <div class="col-sm-2" style=" margin-top: 10px;padding:0px">
                                </div>
                                <div class="col-sm-2" style=" margin-top: 10px;">
                                    <h6>Saturday</h6>
                                </div>
                                @if (Model.Business.Timings.Saturday.Closed != true)
                                {
                                    <div class="col-sm-8" style=" margin-top: 10px;">
                                        @Model.Business.Timings.Saturday.StartTime To  @Model.Business.Timings.Saturday.EndTime
                                    </div>
                                }
                                else
                                {
                                    <div class="col-sm-8">
                                        Closed
                                    </div>
                                }
                                <div class="col-sm-2" style=" margin-top: 10px;padding:0px">

                                </div>
                                <div class="col-sm-2" style=" margin-top: 10px;">
                                    <h6>Sunday</h6>
                                </div>
                                @if (Model.Business.Timings.Sunday.Closed != true)
                                {
                                    <div class="col-sm-8" style=" margin-top: 10px;">
                                        @Model.Business.Timings.Sunday.StartTime To  @Model.Business.Timings.Sunday.EndTime
                                    </div>
                                }
                                else
                                {
                                    <div class="col-sm-8">
                                        Closed
                                    </div>
                                }
                            </div>
                            <div class="form-group row" id="Timings2" style="display:none">
                                <div class="col-sm-2">
                                </div>
                                <div class="col-sm-10" style=" margin-top: 10px;padding:0px">
                                    <table class="table">
                                        <thead>
                                            <tr>
                                                <th></th>
                                                <th>Start Time</th>
                                                <th>End Time</th>
                                                <th>Closed</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td>Monday</td>
                                                <td><input type="time" asp-for="@Model.Business.Timings.Monday.StartTime" onchange="monStart()" id="mondayStartTime" class="input-xlarge"></td>
                                                <td><input type="time" asp-for="@Model.Business.Timings.Monday.EndTime" onchange="monEnd()" id="mondayEndTime" class="input-xlarge"></td>
                                                <td><input asp-for="@Model.Business.Timings.Monday.Closed" type="checkbox" onchange="monClosed()" id="mondayClose" class="input-xlarge"></td>
                                            </tr>
                                            <tr>
                                                <td>Tuesday</td>
                                                <td><input type="time" asp-for="@Model.Business.Timings.Tuesday.StartTime" onchange="tueStart()" id="tuesdayStartTime" class="input-xlarge"></td>
                                                <td><input type="time" asp-for="@Model.Business.Timings.Tuesday.EndTime" onchange="tueEnd()" id="tuesdayEndTime" class="input-xlarge"></td>
                                                <td><input asp-for="@Model.Business.Timings.Tuesday.Closed" type="checkbox" onchange="tueClosed()" id="tuesdayClose" class="input-xlarge"></td>
                                            </tr>
                                            <tr>
                                                <td>Wednesday</td>
                                                <td><input type="time" asp-for="@Model.Business.Timings.Wednesday.StartTime" onchange="wedStart()" id="wednesdayStartTime" class="input-xlarge"></td>
                                                <td><input type="time" asp-for="@Model.Business.Timings.Wednesday.EndTime" onchange="wedEnd()" id="wednesdayEndTime" class="input-xlarge"></td>
                                                <td><input asp-for="@Model.Business.Timings.Wednesday.Closed" type="checkbox" onchange="wedClosed()" id="wednesdayClose" class="input-xlarge"></td>
                                            </tr>
                                            <tr>
                                                <td>Thursday</td>
                                                <td><input type="time" asp-for="@Model.Business.Timings.Thursday.StartTime" onchange="thuStart()" id="thursdayStartTime" class="input-xlarge"></td>
                                                <td><input type="time" asp-for="@Model.Business.Timings.Thursday.EndTime" onchange="thuEnd()" id="thursdayEndTime" class="input-xlarge"></td>
                                                <td><input asp-for="@Model.Business.Timings.Thursday.Closed" type="checkbox" onchange="thuClosed()" id="thursdayClose" class="input-xlarge"></td>
                                            </tr>
                                            <tr>
                                                <td>Friday</td>
                                                <td><input type="time" asp-for="@Model.Business.Timings.Friday.StartTime" onchange="friStart()" id="fridayStartTime" class="input-xlarge"></td>
                                                <td><input type="time" asp-for="@Model.Business.Timings.Friday.EndTime" onchange="friEnd()" id="fridayEndTime" class="input-xlarge"></td>
                                                <td><input asp-for="@Model.Business.Timings.Friday.Closed" type="checkbox" onchange="friClosed()" id="fridayClose" class="input-xlarge"></td>
                                            </tr>
                                            <tr>
                                                <td>Saturday</td>
                                                <td><input type="time" asp-for="@Model.Business.Timings.Saturday.StartTime" onchange="satStart()" id="saturdayStartTime" class="input-xlarge"></td>
                                                <td><input type="time" asp-for="@Model.Business.Timings.Saturday.EndTime" onchange="satEnd()" id="saturdayEndTime" class="input-xlarge"></td>
                                                <td><input asp-for="@Model.Business.Timings.Saturday.Closed" type="checkbox" onchange="satClosed()" id="saturdayClose" class="input-xlarge"></td>
                                            </tr>
                                            <tr>
                                                <td>Sunday</td>
                                                <td><input type="time" asp-for="@Model.Business.Timings.Sunday.StartTime" onchange="sunStart()" id="sundayStartTime" class="input-xlarge"></td>
                                                <td><input type="time" asp-for="@Model.Business.Timings.Sunday.EndTime" onchange="sunEnd()" id="sundayEndTime" class="input-xlarge"></td>
                                                <td><input asp-for="@Model.Business.Timings.Sunday.Closed" type="checkbox" onchange="sunClosed()" id="sundayClose" class="input-xlarge"></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <div class="col-sm-2" style=" margin-top: 10px;padding:0px">
                            </div>
                            <div class="col-sm-2" style="margin:0px;padding:0px">
                                <input id="editTimingsButton" type="button" onclick="EditTimings()" class="btn btn-light" value="Edit" style="" />
                                <input id="updateTimingsButton" type="button" onclick="UpdateTimings()" class="btn btn-primary" value="Update" style="display:none" />
                            </div>

                            <div class="col-sm-12">
                                <hr style="border-top: 1px solid #000;">
                            </div>
                            <div id="About" class="col-sm-10" style="background-color:white">
                                <h3 style="color:blue; font-size:24px">About</h3>
                            </div>
                            <div class="col-sm-2" style="margin:0px;padding:0px">
                                <input id="editAboutButton" type="button" onclick="EditAbout()" class="btn btn-light" value="Edit" style="" />
                                <input id="updateAboutButton" type="button" onclick="UpdateAbout()" class="btn btn-primary" value="Update" style="display:none" />
                            </div>
                            <div id="About" class="col-sm-12" style="background-color:white">
                                <h6 id="About1">@Model.Business.About </h6>
                                <textarea id="About2" placeholder="About the shop.." style="display:none" value="@Model.Business.About" rows="3" class="form-control"></textarea>
                            </div>
                            <div class="col-sm-12">
                                <hr style="border-top: 1px solid #000;">
                            </div>
                            <div id="Products" class="col-sm-10">
                                <h3 style="color:blue; font-size:24px; background-color:white">Products</h3>
                            </div>
                            <div class="col-sm-2" style="margin:0px;padding:0px">
                                <input id="editProductsButton" type="button" onclick="EditProducts()" class="btn btn-light" value="Edit" style="" />
                                <input id="updateProductsButton" type="button" onclick="UpdateProducts()" class="btn btn-primary" value="Update" style="display:none" />
                            </div>
                            <div id="Products" class="col-sm-12">
                                <h6 id="Products1">@Model.Business.Products </h6>
                                <textarea id="Products2" placeholder="About the shop.." style="display:none" value="@Model.Business.About" rows="3" class="form-control"></textarea>
                            </div>
                            <div class="col-sm-12">
                                <hr style="border-top: 1px solid #000;">
                            </div>
                            <div id="Reviews" class="col-sm-12">
                                <h3 style="color:blue; font-size:24px; background-color:white">Reviews</h3>
                            </div>
                            @if (Model.Business.Reviews.Count != 0)
                            {
                                <div class="col-sm-12" style="height:600px;overflow:scroll">
                                    @foreach (var review in Model.Business.Reviews)
                                    {
                                        <div class="col-sm-12" style="background-color:white">

                                            <div class="card">
                                                <div class="card-header" id="ratingscss">
                                                    @if (review.Rating == 1)
                                                    {
                                                        <span class="fa fa-star checked"></span>
                                                        <span class="fa fa-star"></span>
                                                        <span class="fa fa-star"></span>
                                                        <span class="fa fa-star"></span>
                                                        <span class="fa fa-star"></span>
                                                    }
                                                    @if (review.Rating == 2)
                                                    {
                                                        <span class="fa fa-star checked"></span>
                                                        <span class="fa fa-star checked"></span>
                                                        <span class="fa fa-star"></span>
                                                        <span class="fa fa-star"></span>
                                                        <span class="fa fa-star"></span>
                                                    }
                                                    @if (review.Rating == 3)
                                                    {
                                                        <span class="fa fa-star checked"></span>
                                                        <span class="fa fa-star checked"></span>
                                                        <span class="fa fa-star checked"></span>
                                                        <span class="fa fa-star"></span>
                                                        <span class="fa fa-star"></span>
                                                    }
                                                    @if (review.Rating == 4)
                                                    {
                                                        <span class="fa fa-star checked"></span>
