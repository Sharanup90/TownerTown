<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TownerTown</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
   
</head>
<body style="padding:0px">
    <button class="btn btn-link" style="position: absolute;left: 10px;top: 5px;"><a asp-area="" asp-controller="Home" asp-action="Faq">Need Help?</a></button>
    <button class="btn btn-primary" style="position: absolute;right: 10px;top: 20px;"><a asp-area="" asp-controller="Home" asp-action="LogIn" style="color:white">LogIn</a></button>
    <button class="btn btn-primary" style="position: absolute;right: 100px;top: 20px;"><a asp-area="" asp-controller="ManageBusiness" asp-action="Register" style="color:white">Register</a></button>

    @RenderBody()

    @*<button class="btn btn-primary" style="position: absolute;right: 100px;top: 20px;"><a style="color:white" asp-area="" asp-controller="Home" asp-action="LogIn">Register</a></button>*@




    @*<footer class="border-top footer text-muted">
            <div class="container">
                &copy; 2020 - TownerTown.Web - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            </div>
        </footer>*@
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
